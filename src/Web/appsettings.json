{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "NLog": {
    "throwConfigExceptions": true,
    "variables": {
      "logdir": "/usr/local/xiaohangai/logs"
    },
    "targets": {
      "async": true,
      "logfile": {
        "type": "File",
        "fileName": "${logdir}/xiaohangai.log",
        "archiveFileName": "${logdir}/{#}.log",
        "archiveNumbering": "DateAndSequence",
        "archiveAboveSize": "5242880",
        "archiveDateFormat": "yyyyMMdd",
        "maxArchiveFiles": "10",
        "layout": "${longdate}|${uppercase:${level}}|${logger}|${callsite-linenumber}|${message} ${exception:format=tostring}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Information",
        "writeTo": "logfile"
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=/usr/local/xiaohangai/xiaohangai.db"
  },
  "Jwt": {
    "SecretKey": "xiaohang-ai-key",
    "Issuer": "zhangxiaohang",
    "Audience": "xiaohang-ai",
    "Expires": 7200
  }
}